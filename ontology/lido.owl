#    Classes
#################################################################

###  http://example.org/lido#Abbreviation
:Abbreviation rdf:type owl:Class ;
              rdfs:comment "Abbreviations are short forms of linguistic terms which are useful for instance for morphemic glossing or part of speech annotations."@en ;
              rdfs:label "Abbreviation"@en .


###  http://example.org/lido#Area
:Area rdf:type owl:Class ;
      rdfs:comment "A geographic, cultural-geographic or political area."@en ;
      rdfs:label "Area"@en .


###  http://example.org/lido#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf foaf:Person ;
        rdfs:comment "An author of a document."@en ;
        rdfs:label "Author"@en .


###  http://example.org/lido#Concept
:Concept rdf:type owl:Class ;
         rdfs:comment "A  concept is a mental object which is realized by a linguistic sign, which is then the term associated with the concept."@en ;
         rdfs:label "Concept"@en .


###  http://example.org/lido#Editor
:Editor rdf:type owl:Class ;
        rdfs:subClassOf foaf:Person ;
        rdfs:comment "The editor is the person who edits a Concept or Term resource."@en ;
        rdfs:label "Editor"@en .


###  http://example.org/lido#Journal
:Journal rdf:type owl:Class ;
         rdfs:subClassOf bibo:Document ;
         rdfs:comment "A journal is a periodical publication in which scholarship relating to a particular academic discipline is published."@en ;
         rdfs:label "Journal"@en .


###  http://example.org/lido#Language
:Language rdf:type owl:Class ;
          rdfs:label "Language"@en .


###  http://example.org/lido#PublicationPlace
:PublicationPlace rdf:type owl:Class ;
                  rdfs:comment "The location where a document is produced/published."@en ;
                  rdfs:label "Publication Place"@en .


###  http://example.org/lido#Publisher
:Publisher rdf:type owl:Class ;
           rdfs:comment "A publisher is a person or company whose business is the publishing of books, periodicals, etc."@en ;
           rdfs:label "Publisher"@en .


###  http://example.org/lido#Series
:Series rdf:type owl:Class ;
        rdfs:comment "A loose, thematic collection of documents."@en ;
        rdfs:label "Series"@en .


###  http://example.org/lido#Term
:Term rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :term-conceptRelation ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Concept
                      ] ;
      rdfs:comment "A term can be any expression that designates a concept relevant to the domain of linguistics."@en ;
      rdfs:label "Term"@en .


###  http://example.org/lido#Textsort
:Textsort rdf:type owl:Class ;
          rdfs:comment "The sort (i.e. the literary genre) of a document."@en ;
          rdfs:label "Textsort"@en .


###  http://lexvo.org/ontology#Language
lexvo:Language rdf:type owl:Class .


###  http://purl.org/dc/terms/Agent
